
<?xml version="1.0" encoding="UTF-8"?>

<config>
	<Canvas height="600" n_coll="12" n_row="12" name="MomentumResolution" width="1000">
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">
			<Export url="MuPtRes.pdf"/>
			<Export url="MuPtRes.png"/>
			<Histo name="mMuPtResRc" style="style.line">
				<style draw="" linecolor="#0000FF" optstat="1111" title="Momentum Resolution (Muons only); (p^{MC}_{T} - p^{RC}_{T})/(p^{MC}_{T});"/>
			</Histo>
		</Pad>
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">
			<Export url="Pt_Mum.pdf"/>
			<Export url="Pt_Mum.png"/>
			<Histo name="mMumMcPt" style="style.line">
				<style draw="" title="#mu-; p_{T}; dN/dp_{T}"/>
				<style draw="same"/>
				<style draw="same"/>
			</Histo>
			<Histo name="mMumRcPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Histo name="mMumMtdPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Legend columns="3">
				<Entry linewidth="4" markersize="" name="mMumMcPt" opt="l" title="MC"/>
				<Entry linewidth="4" markersize="" name="mMumRcPt" opt="l" title="RC"/>
				<Entry linewidth="4" markersize="" name="mMumMtdPt" opt="l" title="MTD"/>
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99"/>
			</Legend>
		</Pad>
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">
			<Export url="Pt_Mup.pdf"/>
			<Export url="Pt_Mup.png"/>
			<Histo name="mMupMcPt" style="style.line">
				<style draw="" title="#mu+; p_{T}; dN/dp_{T}"/>
				<style draw="same"/>
				<style draw="same"/>
			</Histo>
			<Histo name="mMupRcPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Histo name="mMupMtdPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Legend columns="3">
				<Entry linewidth="4" markersize="" name="mMupMcPt" opt="l" title="MC"/>
				<Entry linewidth="4" markersize="" name="mMupRcPt" opt="l" title="RC"/>
				<Entry linewidth="4" markersize="" name="mMupMtdPt" opt="l" title="MTD"/>
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99"/>
			</Legend>
		</Pad>
	</Canvas>
	<Canvas height="600" n_coll="12" n_row="12" name="Momentum" width="1000">
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">
			<Export url="Pt_Mum.pdf"/>
			<Export url="Pt_Mum.png"/>
			<Histo name="mMumMcPt" style="style.line">
				<style draw="" title="#mu-; p_{T}; dN/dp_{T}"/>
				<style draw="same"/>
				<style draw="same"/>
			</Histo>
			<Histo name="mMumRcPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Histo name="mMumMtdPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Legend columns="3">
				<Entry linewidth="4" markersize="" name="mMumMcPt" opt="l" title="MC"/>
				<Entry linewidth="4" markersize="" name="mMumRcPt" opt="l" title="RC"/>
				<Entry linewidth="4" markersize="" name="mMumMtdPt" opt="l" title="MTD"/>
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99"/>
			</Legend>
		</Pad>
	</Canvas>
	<Canvas height="600" n_coll="12" n_row="12" width="1000">
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">
			<Export url="Pt_Mup.pdf"/>
			<Export url="Pt_Mup.png"/>
			<Histo name="mMupMcPt" style="style.line">
				<style draw="" title="#mu+; p_{T}; dN/dp_{T}"/>
				<style draw="same"/>
				<style draw="same"/>
			</Histo>
			<Histo name="mMupRcPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Histo name="mMupMtdPt" style="style.line">
				<style draw="same"/>
			</Histo>
			<Legend columns="3">
				<Entry linewidth="4" markersize="" name="mMupMcPt" opt="l" title="MC"/>
				<Entry linewidth="4" markersize="" name="mMupRcPt" opt="l" title="RC"/>
				<Entry linewidth="4" markersize="" name="mMupMtdPt" opt="l" title="MTD"/>
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99"/>
			</Legend>
		</Pad>
	</Canvas>
	<Plot name="MomentumResolution">
		<Histo bins_x="bins.ptRes" data="d" draw=" (McTracks[ Tracks.mMcIndex  ].mPt - Tracks.mPt) / (McTracks[ Tracks.mMcIndex  ].mPt) " name="mMuPtResRc" select="Tracks.mMcIndex >= 0 && (McTracks[ Tracks.mMcIndex  ].mGeantPID == 5 || McTracks[ Tracks.mMcIndex  ].mGeantPID == 6)">
			<style optstat="1111"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="McTracks.mPt" name="mMupMcPt" select="McTracks.mGeantPID == 5" style="style.line">
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
			<style draw="same" linecolor="#0000FF"/>
			<style draw="same" linecolor="#FF0000"/>
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
			<style draw="same" linecolor="#0000FF"/>
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMupRcPt" select="Tracks.mMtdPidTraitsIndex >= -1 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 5" style="style.line">
			<style draw="same" linecolor="#0000FF"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMupMtdPt" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 5" style="style.line">
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="McTracks.mPt" name="mMumMcPt" select="McTracks.mGeantPID == 6" style="style.line">
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMumRcPt" select="Tracks.mMtdPidTraitsIndex >= -1 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 6" style="style.line">
			<style draw="same" linecolor="#0000FF"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMumMtdPt" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 6" style="style.line">
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.ptRes" data="d" draw=" (McTracks[ Tracks.mMcIndex  ].mPt - Tracks.mPt) / (McTracks[ Tracks.mMcIndex  ].mPt) " name="mMuPtResRc" select="Tracks.mMcIndex >= 0 && (McTracks[ Tracks.mMcIndex  ].mGeantPID == 5 || McTracks[ Tracks.mMcIndex  ].mGeantPID == 5)"/>
		<Histo bins_x="bins.ptRes" data="d" draw=" (McTracks[ Tracks.mMcIndex  ].mPt - Tracks.mPt) / (McTracks[ Tracks.mMcIndex  ].mPt) " name="mMuPtResMtd" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && (McTracks[ Tracks.mMcIndex  ].mGeantPID == 5 || McTracks[ Tracks.mMcIndex  ].mGeantPID == 5)"/>
		<Histo name="not much"/>
	</Plot>
	<Plot name="Momentum">
		<Histo bins_x="bins.pt" data="d" draw="McTracks.mPt" name="mMupMcPt" select="McTracks.mGeantPID == 5" style="style.line">
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
			<style draw="same" linecolor="#0000FF"/>
			<style draw="same" linecolor="#FF0000"/>
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
			<style draw="same" linecolor="#0000FF"/>
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMupRcPt" select="Tracks.mMtdPidTraitsIndex >= -1 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 5" style="style.line">
			<style draw="same" linecolor="#0000FF"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMupMtdPt" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 5" style="style.line">
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="McTracks.mPt" name="mMumMcPt" select="McTracks.mGeantPID == 6" style="style.line">
			<style draw="" linecolor="#000000" xr="0, 10" yr="1, 8e3"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMumRcPt" select="Tracks.mMtdPidTraitsIndex >= -1 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 6" style="style.line">
			<style draw="same" linecolor="#0000FF"/>
		</Histo>
		<Histo bins_x="bins.pt" data="d" draw="Tracks.mPt" name="mMumMtdPt" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && McTracks[ Tracks.mMcIndex  ].mGeantPID == 6" style="style.line">
			<style draw="same" linecolor="#FF0000"/>
		</Histo>
		<Histo bins_x="bins.ptRes" data="d" draw=" (McTracks[ Tracks.mMcIndex  ].mPt - Tracks.mPt) / (McTracks[ Tracks.mMcIndex  ].mPt) " name="mMuPtResRc" select="Tracks.mMcIndex >= 0 && (McTracks[ Tracks.mMcIndex  ].mGeantPID == 5 || McTracks[ Tracks.mMcIndex  ].mGeantPID == 5)"/>
		<Histo bins_x="bins.ptRes" data="d" draw=" (McTracks[ Tracks.mMcIndex  ].mPt - Tracks.mPt) / (McTracks[ Tracks.mMcIndex  ].mPt) " name="mMuPtResMtd" select="Tracks.mMtdPidTraitsIndex >= 0 && Tracks.mMcIndex >= 0 && (McTracks[ Tracks.mMcIndex  ].mGeantPID == 5 || McTracks[ Tracks.mMcIndex  ].mGeantPID == 5)"/>
	</Plot>
	<Plot name="daniel">
		<Histo name="not much"/>
	</Plot>
	<Task config="" name="skimmer" nodePath="" type="VegaXmlPlotter"/>
	<jobIndex>
		-1
	</jobIndex>
</config>