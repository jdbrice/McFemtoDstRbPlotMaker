<?xml version="1.0" encoding="UTF-8"?>
<config>

	<Canvas n_coll="12" n_row="12" width="1000" height="600" name="Momentum">
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">

			<Histo name="primaryPosMcPt" style="style.line" >
				<style draw="" title="#mu-; p_{T}; dN/dp_{T}" />
			</Histo>
			<Histo name="primaryPosRcPt" style="style.line" >
				<style draw="same" />
			</Histo>
			<Histo name="primaryPosMtdPt" style="style.line" >
				<style draw="same" />
			</Histo>

			<Legend columns="3">
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99" />
				<Entry name="primaryPosMcPt" title="MC" opt="l" />
				<Entry name="primaryPosRcPt" title="RC" opt="l" />
				<Entry name="primaryPosMtdPt" title="MTD" opt="l" />
			</Legend>

			<Export url="Export/primaryPt_{plcA}.pdf" />
			<Export url="Export/primaryPt_{plcA}.png" />
			<Export url="Export/primaryPt_{plcA}.C" />
		</Pad>
	</Canvas>

	<Canvas n_coll="12" n_row="12" width="1000" height="600" name="Momentum">
		<Pad gx="0, 12" gy="0, 12" m="0.1" name="1" rm="0.01" tm="0.01">

			<Histo name="primaryNegMcPt" style="style.line" >
				<style draw="" title="#mu-; p_{T}; dN/dp_{T}" />
			</Histo>
			<Histo name="primaryNegRcPt" style="style.line" >
				<style draw="same" />
			</Histo>
			<Histo name="primaryNegMtdPt" style="style.line" >
				<style draw="same" />
			</Histo>

			<Legend columns="3">
				<Position x1="0.59" x2="0.99" y1="0.89" y2="0.99" />
				<Entry name="primaryNegMcPt" title="MC" opt="l" />
				<Entry name="primaryNegRcPt" title="RC" opt="l" />
				<Entry name="primaryNegMtdPt" title="MTD" opt="l" />
			</Legend>

			<Export url="Export/primaryPt_{plcB}.pdf" />
			<Export url="Export/primaryPt_{plcB}.png" />
			<Export url="Export/primaryPt_{plcB}.C" />
		</Pad>
	</Canvas>

</config>